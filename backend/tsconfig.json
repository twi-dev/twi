{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "baseUrl": "src",
    "paths": {},
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": false,
    "importHelpers": true,
    "noEmit": false,
    "outDir": "lib",
    "strict": true,
    "lib": [
      "ESNext"
    ]
  },
  "include": [
    "**/*.ts",
    "env.d.ts"
  ],
  "ts-node": {
    "preferTsExts": true,
    "require": [
      "tsconfig-paths/register"
    ]
  }
}
