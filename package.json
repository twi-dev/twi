{
  "type": "module",
  "name": "nuxt-app",
  "private": true,
  "main": ".output/server/index.mjs",
  "scripts": {
    "test": "pnpm vitest run",
    "test-watch": "pnpm vitest",
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.development.local",
    "start": "node --env-file=.env.production.local .",
    "generate": "nuxt generate",
    "preview": "nuxt preview --dotenv .env.production.local",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "prepare": "npx is-in-ci || husky install"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./server/lib/db/cli.ts"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "next"
      ]
    },
    "updateConfig": {
      "ignoreDependencies": [
        "next-auth",
        "vitest",
        "@sidebase/nuxt-auth"
      ]
    }
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@commitlint/types": "18.4.3",
    "@faker-js/faker": "8.3.1",
    "@headlessui/tailwindcss": "0.2.0",
    "@mikro-orm/migrations": "5.9.4",
    "@mikro-orm/seeder": "5.9.4",
    "@nuxt/devtools": "1.0.3",
    "@nuxt/test-utils": "3.8.1",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/tailwindcss": "6.10.1",
    "@sidebase/nuxt-auth": "0.5.0",
    "@testing-library/vue": "8.0.1",
    "@types/bcrypt": "5.0.2",
    "@types/cookie": "0.6.0",
    "@types/fs-extra": "11.0.4",
    "@types/lodash-es": "4.17.12",
    "@types/mime-types": "2.1.4",
    "@types/node": "20.10.0",
    "@types/uuid": "9.0.7",
    "@types/validator": "13.11.7",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "@unhead/vue": "1.8.8",
    "@vitejs/plugin-vue": "4.5.0",
    "cookie": "0.6.0",
    "daisyui": "4.4.10",
    "eslint": "8.54.0",
    "eslint-plugin-vue": "9.18.1",
    "execa": "8.0.1",
    "husky": "8.0.3",
    "jsdom": "23.0.0",
    "lint-staged": "15.1.0",
    "nuxt": "3.8.2",
    "nuxt-svgo": "3.6.0",
    "tailwindcss": "3.3.5",
    "ts-node": "10.9.1",
    "vite": "5.0.2",
    "vitest": "0.34.4",
    "vue": "3.3.9",
    "vue-router": "4.2.5"
  },
  "dependencies": {
    "@decs/typeschema": "0.12.1",
    "@headlessui/vue": "1.7.16",
    "@mikro-orm/cli": "5.9.4",
    "@mikro-orm/core": "5.9.4",
    "@mikro-orm/mysql": "5.9.4",
    "@nuxt/image": "1.1.0",
    "@sindresorhus/slugify": "2.2.1",
    "@trpc/client": "10.44.1",
    "@trpc/server": "10.44.1",
    "@tus/file-store": "1.0.1",
    "@tus/server": "1.0.1",
    "@uppy/core": "3.7.1",
    "@uppy/tus": "3.4.0",
    "@vorms/core": "1.2.0-beta.4",
    "@vorms/resolvers": "1.2.0-beta.4",
    "@vueuse/core": "10.6.1",
    "@vueuse/nuxt": "10.6.1",
    "bcrypt": "5.1.1",
    "cropperjs": "1.6.1",
    "date-fns": "2.30.0",
    "fs-extra": "11.1.1",
    "h3": "1.9.0",
    "lodash-es": "4.17.21",
    "lucide-vue-next": "0.293.0",
    "mime-types": "2.1.35",
    "mysql2": "3.6.5",
    "nanoid": "5.0.3",
    "next-auth": "4.22.5",
    "sharp": "0.32.6",
    "superjson": "2.2.1",
    "trpc-nuxt": "0.10.12",
    "type-is": "1.6.18",
    "uuid": "9.0.1",
    "valibot": "0.21.0",
    "validator": "13.11.0"
  }
