{
  "type": "module",
  "name": "nuxt-app",
  "private": true,
  "main": ".output/server/index.mjs",
  "scripts": {
    "test": "pnpm vitest run",
    "test-watch": "pnpm vitest",
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.development.local",
    "generate": "nuxt generate",
    "preview": "nuxt preview --dotenv .env.production.local",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "prepare": "husky install"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./server/lib/db/cli.ts"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "next"
      ]
    },
    "updateConfig": {
      "ignoreDependencies": [
        "next-auth"
      ]
    }
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@commitlint/types": "17.4.4",
    "@faker-js/faker": "8.0.2",
    "@headlessui/tailwindcss": "0.2.0",
    "@mikro-orm/migrations": "5.8.1",
    "@mikro-orm/seeder": "5.8.1",
    "@nuxt/devtools": "0.8.3",
    "@nuxt/test-utils": "3.7.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/tailwindcss": "6.8.0",
    "@sidebase/nuxt-auth": "0.5.0",
    "@testing-library/vue": "7.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/cookie": "0.5.2",
    "@types/lodash-es": "4.17.9",
    "@types/mime-types": "2.1.1",
    "@types/node": "20.6.2",
    "@types/validator": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "4.3.4",
    "cookie": "0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "execa": "8.0.1",
    "husky": "^8.0.0",
    "jsdom": "22.1.0",
    "nuxt": "3.7.3",
    "nuxt-svgo": "3.5.3",
    "ts-node": "10.9.1",
    "vitest": "0.34.4"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.16",
    "@mikro-orm/cli": "5.8.1",
    "@mikro-orm/core": "5.8.1",
    "@mikro-orm/mysql": "5.8.1",
    "@sindresorhus/slugify": "2.2.1",
    "@trpc/client": "10.38.3",
    "@trpc/server": "10.38.3",
    "@tus/file-store": "1.0.0",
    "@tus/server": "1.0.0",
    "@uppy/core": "3.4.0",
    "@uppy/tus": "3.1.3",
    "@vorms/core": "1.1.0",
    "@vorms/resolvers": "1.1.0",
    "@vueuse/core": "10.4.1",
    "@vueuse/nuxt": "10.4.1",
    "bcrypt": "5.1.1",
    "cropperjs": "1.6.1",
    "date-fns": "2.30.0",
    "lodash-es": "4.17.21",
    "lucide-vue-next": "0.279.0",
    "mime-types": "2.1.35",
    "mysql2": "3.6.1",
    "nanoid": "5.0.1",
    "next-auth": "4.22.5",
    "superjson": "1.13.1",
    "trpc-nuxt": "0.10.12",
    "validator": "13.11.0",
    "zod": "3.22.2"
  }
}
