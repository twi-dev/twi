{
  "type": "module",
  "name": "nuxt-app",
  "private": true,
  "main": ".output/server/index.mjs",
  "scripts": {
    "test": "pnpm vitest run",
    "test-watch": "pnpm vitest",
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.development.local",
    "generate": "nuxt generate",
    "preview": "nuxt preview --dotenv .env.production.local",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./server/lib/db/cli.ts"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "next"
      ]
    },
    "updateConfig": {
      "ignoreDependencies": [
        "next-auth"
      ]
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@mikro-orm/migrations": "5.7.14",
    "@mikro-orm/seeder": "5.7.14",
    "@nuxt/devtools": "0.8.0",
    "@nuxt/test-utils": "3.7.0",
    "@nuxtjs/tailwindcss": "6.8.0",
    "@sidebase/nuxt-auth": "0.5.0",
    "@testing-library/vue": "5",
    "@types/bcrypt": "5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/lodash-es": "4.17.8",
    "@types/node": "18.17.3",
    "@types/validator": "13.11.1",
    "@vitejs/plugin-vue": "4.3.3",
    "cookie": "^0.5.0",
    "execa": "8.0.1",
    "jsdom": "22.1.0",
    "nuxt": "3.7.0",
    "ts-node": "10.9.1",
    "vitest": "0.34.3"
  },
  "dependencies": {
    "@mikro-orm/cli": "5.7.14",
    "@mikro-orm/core": "5.7.14",
    "@mikro-orm/mysql": "5.7.14",
    "@sindresorhus/slugify": "2.2.1",
    "@trpc/client": "10.37.1",
    "@trpc/server": "10.37.1",
    "@vorms/core": "1.1.0",
    "@vorms/resolvers": "1.1.0",
    "bcrypt": "5.1.1",
    "date-fns": "2.30.0",
    "lodash-es": "4.17.21",
    "lucide-vue-next": "^0.270.0",
    "mysql2": "3.6.0",
    "next-auth": "4.22.5",
    "superjson": "1.13.1",
    "trpc-nuxt": "0.10.10",
    "validator": "13.11.0",
    "zod": "3.22.2"
  }
}
